
public class UnaryOperator {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
        int x = 1;
        int y = 1;
        
        int nextx = ++x; //2
        int nexty = y++; //1
        
        System.out.println(nextx);
        System.out.println(nexty);
	}

}
