
public class Expression2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
        int x = 10;
        int y = 20;
        double z = 2.0;
        double k = 0.0;
        int sum;
        double devide;
        
        sum = x+y;
        devide = z/k;
        
        System.out.println(sum);
        System.out.println(devide);
	}

}
