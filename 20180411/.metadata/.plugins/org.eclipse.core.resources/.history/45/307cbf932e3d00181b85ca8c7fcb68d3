import java.util.Scanner;
public class Score2Gradle {
	public static void main(String[] args) {
		int score, number;
		char grade;
		Scanner scan = new Scanner(System.in);
		System.out.print("성적을 입력하시오: ");
        score = scan.nextInt();
        number = score/10;
        switch(number) {
        //case 10:
        case 9:
        	grade = 'A';
        	break;
        case 8:
        	grade = 'B';
        	break;
        case 7:
        	grade = 'C';
        	break;
        case 6:
        	grade = 'D';
        	break;
        case 5:
        	grade = 'F';
        	break;
        case 4:
        	grade = 'G';
        	break;
        default:
        	grade = 'H';
        	break;
        }
        System.out.print("학점: "+grade);
        scan.close();
	}

}
